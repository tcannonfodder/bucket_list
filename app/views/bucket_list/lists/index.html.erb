<h1>Lists#index</h1>
<ul>
<% @lists.each do |list| %>
	<li><%= link_to list.name, list %> <%= link_to "Edit", edit_list_path(list) %> <%= link_to "Delete", list_path(list), :method => :delete %>
		<ul>
		<% list.items.each do |item| %>
			<li><%= item.name %></li>
		<% end %>
		</ul>
	</li>
<% end %>
</ul>